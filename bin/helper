#!/bin/bash

source bin/functions.sh


if [ $# -eq 0 ]; then
    renderCommands;
    exit 0;
fi


case "$1" in
    start)
        start;
        ;;

    stop)
        stop;
        ;;

    in)
        goIntoContainerCurrentUser;
        ;;

    indef)
        goIntoContainerDefaultUser;
        ;;
    *)
        echo "Sorry, I don't know want you want"
esac
